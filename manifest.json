{
  "manifest_version": 2,
  "name": "SplunkExpander",
  "description": "Expand jsexpanders in splunk",
  "version": "1",
  "author": "Jon-Anders Kabbe",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["background.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {},
  "permissions": ["activeTab"]
}